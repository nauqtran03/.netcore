@model IEnumerable<buoi13_netcore.Models.Member>
@{
    ViewData["Title"] = "Information";
}
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Address</th>
            <th>City</th>
            <th>Region</th>
        </tr>
    </thead>
    <tbody>
            @foreach(var member in ViewBag.Members){
                <tr>
                    <td>@member.Id</td>
                    <td>@member.Name</td>
                    <td>@member.Address</td>
                    <td>@member.City</td>
                    <td>@member.Region</td>
                </tr>
            }
    </tbody>
</table>